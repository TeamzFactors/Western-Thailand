<!-- Registration Form -->
<img src="/img/people/sign_view.jpeg" alt="login image" class="sign__img">
<div class="container-fluid">
    <h1 class="form-title">Registration</h1>
    <form action="/register" method="POST">

      <div class="main-user-info">
        <div class="user-input-box">
          <label for="username">Username</label>
          <input type="text"
                  id="input_username"
                  name="username"
                  placeholder="Enter Username" />
        </div>

        <div class="user-input-box">
          <label for="email">Email Address</label>
          <input type="email"
                  id="input_email"
                  name="email"
                  placeholder="Enter Email Address"/>
        </div>

        <div class="user-input-box">
          <label for="password">Password</label>
          <input type="password" id="input-pass" name="password" placeholder="Enter Password"/>
          <i class="ri-eye-off-line login__eye" id="eye-icon-pass"></i>
        </div>
        
        <div class="user-input-box">
          <label for="confirmPassword">Confirm Password</label>
          <input type="password" id="input-confirm-pass" name="confirmPassword" placeholder="Confirm Password"/>
          <i class="ri-eye-off-line login__eye" id="eye-icon-confirm-pass"></i>
        </div>

        <div class="user-input-box">
          <label for="phoneNumber">Phone Number</label>
          <input type="text"
                  id="input_phoneNumber"
                  name="phoneNumber"
                  placeholder="Enter Phone Number"/>
        </div>

        <div class="user-input-box">
            <label for="birthday">Birth Date</label>
            <input type="date"
                    id="input_birthDate"
                    name="birthDate"
                    placeholder=""/>
          </div>
      </div>
      
      <div class="login__check">
        <label for="input-check" class="login__check-label"><input type="checkbox" class="login__check-input" id="input-check"/>I accept terms and conditions.</label>
    </div>
      <div class="form-submit-btn">
        <input type="submit" value="register" onclick="">
      </div>
      <div class="need__login">
        <b>Already have an Account? <a href="/logins" class="want__login">Login Now!</a></b>
        </div>
    </form>
</div>

